<% if(typeof user == "undefined") { %>
    <div class="userLogo">
        <a class="profileLink" href="/login">
            <span>Login</span>
        </a>
    </div>
<% }else{ %>
    <div class="userLogo" onclick="toggleMenu()">
        <a class="profileLink" href="#">
            <img src="<%= user.profilePic %>" referrerPolicy="no-referrer" alt="user profile image">
            <span><%= user.userName %></span>
        </a>
    </div>
<% } %>